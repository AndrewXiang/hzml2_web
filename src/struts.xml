<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
 <package name="default" extends="struts-default">
         <action name="login" class="com.web.struts.action.LoginAction">
             <result name="success">/web/main.jsp</result>
             <result name="error">error.html</result>
         </action>
         <action name="addUser" class="com.web.struts.action.AddUserAction">
             <result name ="success">/web/Information.jsp</result>
             <result name ="input">/web/userManage.jsp</result>
         </action>
         <action name="editUser" class="com.web.struts.action.EditUserAction">
             <result name ="success">/web/Information.jsp</result>
             <result name ="input">/web/pwdModify.jsp</result>
         </action>
    </package>
</struts>  